% 性能測定用コード
% 初期状態に「tN, tN, tN.」を付加してやると，s ^ (2 * (N + 1)) ステップになるはず

% t3, t3, t3. % ----> 256 steps

t11@@ t11 :- t10, t10, t10, t10.
t10@@ t10 :- t9, t9, t9, t9.
t9@@  t9  :- t8, t8, t8, t8.
t8@@  t8  :- t7, t7, t7, t7.
t7@@  t7  :- t6, t6, t6, t6.
t6@@  t6  :- t5, t5, t5, t5.
t5@@  t5  :- t4, t4, t4, t4.
t4@@  t4  :- t3, t3, t3, t3.
t3@@  t3  :- t2, t2, t2, t2.
t2@@  t2  :- t1, t1, t1, t1.
t1@@  t1  :- s0, t0, s0, t0.

res = n.
s0@@ s0, res = S :- res = s(S).
t0@@ t0, R = s(S) :- R = S.

